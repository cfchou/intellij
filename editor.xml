<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
    <option name="IS_DND_ENABLED" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bNOTE\b.*">
      <option name="FOREGROUND" value="ba3bc0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bQUERY\b.*">
      <option name="FOREGROUND" value="c02528" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="ffffff" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
  </component>
</application>